{
  "name": "gatsby-web3-starter",
  "description": "A template repository for the purpose of quickly creating a blockchain enabled website.",
  "version": "0.0.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "commit": "git-cz"
  },
  "dependencies": {
    "@loadable/component": "5.15.0",
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "autoprefixer": "^10.2.6",
    "gatsby": "4.1.4",
    "gatsby-plugin-image": "2.1.2",
    "gatsby-plugin-manifest": "4.1.3",
    "gatsby-plugin-offline": "5.1.3",
    "gatsby-plugin-postcss": "5.1.0",
    "gatsby-plugin-preact": "6.1.0",
    "gatsby-plugin-purgecss": "6.1.0",
    "gatsby-plugin-react-helmet": "5.1.0",
    "gatsby-plugin-sharp": "4.1.3",
    "gatsby-source-filesystem": "4.1.2",
    "gatsby-transformer-sharp": "4.1.0",
    "node-polyfill-webpack-plugin": "1.1 .4",
    "postcss": "^8.3.4",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "2.2.19",
    "web3": "1.6.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "electron": "15.3.1",
    "prettier": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redhair/gatsby-web3-starter"
  },
  "bugs": {
    "url": "https://github.com/redhair/gatsby-web3-starter/issues"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run clean && npm run build"
    }
  }
}
